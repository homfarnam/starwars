{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn codegen && next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "schemagen": "apollo client:download-schema --endpoint='https://fe-case-study.vercel.app/api/graphql' schema.graphqls",
    "codegen": "yarn schemagen && graphql-let",
    "types": "apollo client:codegen --localSchemaFile=./schema.graphqls --includes=./graphql/**/*.ts --target=typescript",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "latest",
    "@tailwindcss/forms": "^0.3.2",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/store": "^2.0.2",
    "@types/styled-components": "^5.1.9",
    "apollo": "^2.33.4",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "graphql-let": "^0.18.1",
    "graphql-tag": "^2.12.4",
    "isomorphic-unfetch": "^3.1.0",
    "next": "latest",
    "next-with-apollo": "^5.1.1",
    "prettier": "^2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "store": "^2.0.12",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/import-types-preset": "^1.18.2",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.0",
    "tailwindcss": "^2.1.2",
    "typescript": "4.0"
  },
  "license": "MIT"
}
